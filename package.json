{
  "name": "typeybara",
  "displayName": "Typeybara",
  "description": "Capybara running with your typing",
  "version": "0.1.0",
  "publisher": "openkmj",
  "engines": {
    "vscode": "^1.86.0"
  },
  "author": {
    "name": "openkmj"
  },
  "homepage": "https://github.com/openkmj/typeybara",
  "repository": {
    "type": "git",
    "url": "https://github.com/openkmj/typeybara.git"
  },
  "icon": "img/logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "keywords": [
    "capybara",
    "wpm",
    "vscode"
  ],
  "license": "Apache-2.0",
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "capybara-0": {
        "description": "capybara 0",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E905"
        }
      },
      "capybara-1": {
        "description": "capybara 1",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E904"
        }
      },
      "capybara-2": {
        "description": "capybara 2",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E903"
        }
      },
      "capybara-3": {
        "description": "capybara 3",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E902"
        }
      },
      "capybara-4": {
        "description": "capybara 4",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E901"
        }
      },
      "capybara-5": {
        "description": "capybara 0",
        "default": {
          "fontPath": "runner.woff",
          "fontCharacter": "\\E904"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
